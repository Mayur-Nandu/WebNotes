{
    "manifest_version":2,
    "name":"WebNotes",
    "version":"1.0",
    "description":"web based notes for websites",
    "icons":{
        "128":"icon128.png",
        "48":"icon48.png",
        "16":"icon16.png"
    },
    "content_scripts":[
        {
            "matches":["<all_urls>"],
            "js":["content.js"]
        }
    ],
    "browser_action":{
        "default_icon":"icon48.png",
        "default_title": "this is a popup"
    },
    "background":{
        "scripts":["eventPage.js"]
    }
}